[
  {
    "id": "group_population",
    "type": "group",
    "label": "一、人口与入园情况",
    "description": "普及普惠水平相关基础数据",
    "width": "100%",
    "required": true,
    "children": [
      {
        "id": "age3_6_population",
        "type": "number",
        "label": "户籍所在地3-6岁人口数",
        "placeholder": "请输入户籍所在地3-6岁人口数",
        "width": "33%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "人",
        "elementCode": "E001",
        "helpText": "用于计算学前三年毛入园率的分母"
      },
      {
        "id": "student_total",
        "type": "number",
        "label": "实际在园人数",
        "placeholder": "请输入实际在园人数",
        "width": "33%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "人",
        "elementCode": "E002",
        "helpText": "当地在册幼儿园3-6岁在园幼儿数"
      },
      {
        "id": "enrollment_rate",
        "type": "number",
        "label": "学前三年毛入园率",
        "placeholder": "系统自动计算",
        "width": "34%",
        "required": true,
        "readonly": true,
        "computed": true,
        "decimalPlaces": "2位小数",
        "unit": "%",
        "elementCode": "D001",
        "formula": "student_total / age3_6_population * 100",
        "threshold": ">= 85%",
        "helpText": "（实际在园人数 ÷ 户籍所在地3-6岁人口数）× 100，应≥85%"
      },
      {
        "id": "divider_coverage",
        "type": "divider",
        "label": "普惠性覆盖情况",
        "width": "100%",
        "required": false
      },
      {
        "id": "public_kindergarten_student_count",
        "type": "number",
        "label": "公办园在园幼儿数",
        "placeholder": "请输入公办园在园幼儿数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "人",
        "elementCode": "E003",
        "helpText": "当地公办园(含公办教学点)3-6岁在园幼儿数"
      },
      {
        "id": "inclusive_private_student_count",
        "type": "number",
        "label": "普惠性民办园在园幼儿数",
        "placeholder": "请输入普惠性民办园在园幼儿数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "人",
        "elementCode": "E004",
        "helpText": "普惠性民办园3-6岁在园幼儿数"
      },
      {
        "id": "inclusive_coverage_rate",
        "type": "number",
        "label": "普惠性幼儿园覆盖率",
        "placeholder": "系统自动计算",
        "width": "25%",
        "required": true,
        "readonly": true,
        "computed": true,
        "decimalPlaces": "2位小数",
        "unit": "%",
        "elementCode": "D002",
        "formula": "(public_kindergarten_student_count + inclusive_private_student_count) / student_total * 100",
        "threshold": ">= 80%",
        "helpText": "（公办园+普惠性民办园在园幼儿数）÷ 在园幼儿总数 × 100，应≥80%"
      },
      {
        "id": "public_kindergarten_ratio",
        "type": "number",
        "label": "公办园在园幼儿占比",
        "placeholder": "系统自动计算",
        "width": "25%",
        "required": true,
        "readonly": true,
        "computed": true,
        "decimalPlaces": "2位小数",
        "unit": "%",
        "elementCode": "D003",
        "formula": "public_kindergarten_student_count / student_total * 100",
        "threshold": ">= 50%",
        "helpText": "公办园在园幼儿数 ÷ 在园幼儿总数 × 100，应≥50%"
      }
    ]
  },
  {
    "id": "group_kindergarten_stats",
    "type": "group",
    "label": "二、幼儿园与教职工情况",
    "description": "保教质量相关基础数据",
    "width": "100%",
    "required": true,
    "children": [
      {
        "id": "kindergarten_total",
        "type": "number",
        "label": "幼儿园总数",
        "placeholder": "请输入幼儿园总数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "所",
        "elementCode": "E005"
      },
      {
        "id": "staff_qualified_kindergarten_count",
        "type": "number",
        "label": "教职工配备达标幼儿园数",
        "placeholder": "请输入教职工配备达标幼儿园数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "所",
        "elementCode": "E010",
        "helpText": "按《幼儿园教职工配备标准(暂行)》配足配齐各类教职工的幼儿园数"
      },
      {
        "id": "staff_qualified_rate",
        "type": "number",
        "label": "教职工配备达标率",
        "placeholder": "系统自动计算",
        "width": "25%",
        "required": true,
        "readonly": true,
        "computed": true,
        "decimalPlaces": "2位小数",
        "unit": "%",
        "elementCode": "D006",
        "formula": "staff_qualified_kindergarten_count / kindergarten_total * 100",
        "threshold": ">= 80%",
        "thresholdType": "tiered",
        "helpText": "100%为合格，80%以上为基本合格"
      },
      {
        "id": "fulltime_teacher_count",
        "type": "number",
        "label": "专任教师总数",
        "placeholder": "请输入专任教师总数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "人",
        "elementCode": "E011",
        "helpText": "县域内幼儿园专任教师总数"
      },
      {
        "id": "divider_class",
        "type": "divider",
        "label": "班级情况",
        "width": "100%",
        "required": false
      },
      {
        "id": "class_total",
        "type": "number",
        "label": "班级总数",
        "placeholder": "请输入班级总数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "个",
        "elementCode": "E008",
        "helpText": "县域内幼儿园班级总数"
      },
      {
        "id": "class_size_qualified_count",
        "type": "number",
        "label": "班额符合规定的班级数",
        "placeholder": "请输入班额符合规定的班级数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "个",
        "elementCode": "E009",
        "helpText": "符合《幼儿园工作规程》班额规定的班级数"
      },
      {
        "id": "class_size_qualified_rate",
        "type": "number",
        "label": "班额符合规定比例",
        "placeholder": "系统自动计算",
        "width": "25%",
        "required": true,
        "readonly": true,
        "computed": true,
        "decimalPlaces": "2位小数",
        "unit": "%",
        "elementCode": "D005",
        "formula": "class_size_qualified_count / class_total * 100",
        "threshold": ">= 85%",
        "helpText": "（班额符合规定的班级数 ÷ 班级总数）× 100"
      },
      {
        "id": "teacher_student_ratio",
        "type": "text",
        "label": "师幼比",
        "placeholder": "如1:12",
        "width": "25%",
        "required": true,
        "elementCode": "E204",
        "threshold": "<= 1:15",
        "helpText": "县域内幼儿园专任教师总数与在园幼儿总数之比"
      }
    ]
  },
