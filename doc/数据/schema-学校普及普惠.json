[
  {
    "id": "group_basic_info",
    "type": "group",
    "label": "一、基本信息",
    "width": "100%",
    "required": true,
    "children": [
      {
        "id": "kindergarten_name",
        "type": "text",
        "label": "幼儿园名称",
        "placeholder": "请输入幼儿园全称",
        "width": "50%",
        "required": true
      },
      {
        "id": "kindergarten_code",
        "type": "text",
        "label": "幼儿园代码",
        "placeholder": "请输入幼儿园代码",
        "width": "50%",
        "required": true
      },
      {
        "id": "kindergarten_type",
        "type": "select",
        "label": "办园性质",
        "width": "33%",
        "required": true,
        "options": [
          { "label": "教育部门办园", "value": "education_dept" },
          { "label": "其他部门办园", "value": "other_dept" },
          { "label": "地方企业办园", "value": "local_enterprise" },
          { "label": "事业单位办园", "value": "institution" },
          { "label": "部队办园", "value": "military" },
          { "label": "集体办园", "value": "collective" },
          { "label": "普惠性民办园", "value": "inclusive_private" },
          { "label": "非普惠性民办园", "value": "non_inclusive_private" }
        ]
      },
      {
        "id": "is_residential_supporting",
        "type": "select",
        "label": "是否小区配套幼儿园",
        "width": "33%",
        "required": true,
        "options": [
          { "label": "是", "value": "yes" },
          { "label": "否", "value": "no" }
        ]
      },
      {
        "id": "founding_year",
        "type": "date",
        "label": "开园时间（年）",
        "placeholder": "请选择开园年份",
        "width": "34%",
        "required": true
      },
      {
        "id": "residential_area_name",
        "type": "text",
        "label": "小区名称",
        "placeholder": "请输入配套小区名称",
        "width": "50%",
        "required": false,
        "showWhen": {
          "field": "is_residential_supporting",
          "value": "yes"
        }
      },
      {
        "id": "residential_area_address",
        "type": "text",
        "label": "小区地址",
        "placeholder": "请输入小区地址",
        "width": "50%",
        "required": false,
        "showWhen": {
          "field": "is_residential_supporting",
          "value": "yes"
        }
      }
    ]
  },
  {
    "id": "group_students",
    "type": "group",
    "label": "二、在园幼儿信息",
    "width": "100%",
    "required": true,
    "children": [
      {
        "id": "divider_student_count",
        "type": "divider",
        "label": "在园幼儿人数",
        "width": "100%",
        "required": false
      },
      {
        "id": "total_student_count",
        "type": "number",
        "label": "在园幼儿总数",
        "placeholder": "请输入在园幼儿总数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "人"
      },
      {
        "id": "age3_student_count",
        "type": "number",
        "label": "3岁幼儿人数",
        "placeholder": "请输入3岁幼儿人数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "人"
      },
      {
        "id": "age4_student_count",
        "type": "number",
        "label": "4岁幼儿人数",
        "placeholder": "请输入4岁幼儿人数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "人"
      },
      {
        "id": "age5_student_count",
        "type": "number",
        "label": "5岁幼儿人数",
        "placeholder": "请输入5岁幼儿人数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "人"
      },
      {
        "id": "divider_class_info",
        "type": "divider",
        "label": "班级信息",
        "width": "100%",
        "required": false
      },
      {
        "id": "class_count",
        "type": "number",
        "label": "班级总数",
        "placeholder": "请输入班级总数",
        "width": "25%",
        "required": true,
        "decimalPlaces": "整数",
        "unit": "个"
      },
      {
        "id": "class_student_list",
        "type": "dynamicList",
        "label": "各班级人数",
        "width": "100%",
        "required": true,
        "minRows": 1,
        "maxRows": 100,
        "headers": ["序号", "班级名称", "人数（人）"],
        "fields": [
          {
            "id": "class_index",
            "type": "number",
            "label": "序号",
            "width": "15%",
            "required": true,
            "decimalPlaces": "整数"
          },
          {
            "id": "class_name",
            "type": "text",
            "label": "班级名称",
            "width": "45%",
            "required": true
          },
          {
            "id": "class_student_count",
            "type": "number",
            "label": "人数",
            "width": "40%",
            "required": true,
            "decimalPlaces": "整数",
            "unit": "人"
          }
        ]
      }
    ]
  },
  {
    "id": "group_staff",
