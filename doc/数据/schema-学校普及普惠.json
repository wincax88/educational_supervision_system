  {
    "id": "group_finance",
    "type": "group",
    "label": "五、收费情况",
    "width": "100%",
    "required": true,
    "children": [
      {
        "id": "tuition_fee_type_a",
        "type": "number",
        "label": "保育教育费A类（元/月/生）",
        "placeholder": "请输入A类保育教育费",
        "width": "33%",
        "required": false,
        "decimalPlaces": "2位小数",
        "unit": "元/月/生",
        "helpText": "仅填报收费标准上限"
      },
      {
        "id": "tuition_fee_type_b",
        "type": "number",
        "label": "保育教育费B类（元/月/生）",
